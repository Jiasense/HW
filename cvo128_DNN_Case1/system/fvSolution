FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      fvSolution;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

solvers
{
    "rho.*"
    {
        solver          diagonal;
    }

    p
    {
        solver PCG;
        preconditioner{
            preconditioner GAMG;
            smoother GaussSeidel;
            cacheAgglomeration true;
            agglomerator faceAreaPair;
            mergeLevels 2;
            nVcycles 1;
            // maxLevels 8;
            nCellsInCoarsestLevel 32;
            nPreSweeps 1;
            nPostSweeps 1;
            maxPreSweeps 1;
            maxPostSweeps 1;
            nFinestSweeps 1;
            nCoarsestSweeps 1;
            preSweepsLevelMultiplier 0;
            postSweepsLevelMultiplier 0;
            interpolateCorrection false;
            scaleCorrection true;
            solveCoarsest false;
        }
        maxIter         1000;
        tolerance       1e-7;
        relTol          0;
    }

    pFinal
    {
        solver PCG;
        preconditioner{
            preconditioner GAMG;
            smoother GaussSeidel;
            cacheAgglomeration true;
            agglomerator faceAreaPair;
            mergeLevels 2;
            nVcycles 1;
            // maxLevels 8;
            nCellsInCoarsestLevel 32;
            nPreSweeps 1;
            nPostSweeps 1;
            maxPreSweeps 1;
            maxPostSweeps 1;
            nFinestSweeps 1;
            nCoarsestSweeps 1;
            preSweepsLevelMultiplier 0;
            postSweepsLevelMultiplier 0;
            interpolateCorrection false;
            scaleCorrection true;
            solveCoarsest false;
        }
        maxIter         1000;
        tolerance       1e-7;
        relTol          0;
    }

    "(U|ha|k|epsilon)"
    {
        solver          PBiCGStab;
        preconditioner  none;
        tolerance       1e-9;
        relTol          0;
        maxIter         1000;
    }

    "(U|ha|k|epsilon)Final"
    {
        $U;
        tolerance       1e-9;
        relTol          0;
        maxIter         1000;
    }

    "Yi"
    {
        solver          PBiCGStab;
        preconditioner  none;
        tolerance       1e-9;
        relTol          0;
        maxIter         1000;
    }
    "YiFinal"
    {
        solver          PBiCGStab;
        preconditioner  none;
        tolerance       1e-9;
        relTol          0;
        maxIter         1000;
    }
}

PIMPLE
{
    momentumPredictor yes;
    nOuterCorrectors  1;
    nCorrectors     2;
    nNonOrthogonalCorrectors 0;
}


